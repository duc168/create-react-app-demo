{
    "compilerOptions": {
        "allowJs": true,
        "noImplicitAny": true,
        "outDir": "./ts-build",
        "moduleResolution": "Node",
        "jsx":"react",
        "module": "ESNext",
        "target": "ES5",
        "allowSyntheticDefaultImports": true,
        "baseUrl": ".",
        "paths": {
            "@/*": ["src/*"]
        }
    },
    "include": ["src"]
}